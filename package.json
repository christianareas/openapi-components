{
  "name": "openapi-components",
  "author": "Christian Areas <me@areas.me>",
  "repository": {
    "type": "git",
    "url": "https://github.com/christianareas/openapi-components"
  },
  "license": "GPL-3.0",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc && ncp ./src/types ./dist/types",
    "update-ts": "npm i typescript@latest @types/js-yaml@latest @types/lodash@latest @types/node@latest @types/react@latest @types/react-dom@latest",
    "update-react": "npm i react@latest react-dom@latest",
    "update-tools": "npm i js-yaml@latest lodash@latest ncp@latest",
    "update-all": "npm run update-ts && npm run update-react && npm run update-tools",
    "link": "npm link",
    "relink": "npm unlink -g && npm link",
    "unlink": "npm unlink -g"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "ncp": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
