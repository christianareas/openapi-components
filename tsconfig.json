{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"jsx": "react",
		"outDir": "./dist",
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"@openapi-components": [ "./src/index.ts" ]
		}
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}
